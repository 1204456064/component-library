{
  "name": "project-ts",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "vue-tsc && vite build && node config/package.ts",
    "preview": "vite preview",
    "patch": "node config/updateVersion.ts patch && npm run build"

  },
  "files": [
    "dist"
  ],
  "main": "./dist/cw-component.umd.ts",
