  "exports": {
    ".": {
      "import": "./dist/cw-component.es.ts",
      "require": "./dist/cw-component.umd.ts"
    }
  },
  "dependencies": {
    "ant-design-vue": "^3.2.15",
    "element-plus": "^2.2.28",
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@vitejs/plugin-vue": "^4.0.0",
    "typescript": "^4.9.3",
    "vite": "^4.0.0",
    "vue-tsc": "^1.0.11"
  }
}
